            <div class="row"><!-- main container -->

                    <div style="width: 100%;margin: 0 auto;">

                          <div class="pf_contact_cl01 pf_video_cl09" >
                          <span>OUR VIDEOS</span>
                          </div>
                          <br><br>

                    <div class="videos" id="VideoDiv">      
                    <div class="pf_loader_new" id="pre_video_load">
                    <img src="https://www.parentfinder.com/pf_badge_load/templates/Template_01/images/facebook.gif" class="pf_loader_new_img" alt="Loading.."/>
                    </div>
                    <div id="noVideo" class='noVideo' style="margin: auto; width: 100%; background-color:#E0E1E2">
                    <img class="img-responsive" src='https://www.parentfinder.com/templates/tmpl_par/images/NO-VIDEOS_icon.png' style='margin:auto'/>
                    </div>
                    </div>

                            <div id="carouselV" class="flexslider">
                                <ul class="slides"></ul>
