/********filter menu responsive starts********/
.view #pf_filter {/*filter menu container */
    position:relative;
    height:auto;
    width:300px;
    margin:auto;
    margin-bottom: 85px;
    display:none;

}

.pf_filter_cl01 {/*filter menu container dropdown*/
    height:35px;
    width:300px;
    overflow: hidden;
    position: absolute;
    z-index: 1000;

}

.pf_filter_cl01 .pf_content_menu {/*filter menu links*/
    display:block;
    padding: 0px;
    width: 300px;
    border: 1px solid #a4bed4;
    height:102px;

}

.pf_filter_cl01 .pf_content_menu a {/*filter menu container dropdown*/
    width:100%;
    padding-left: 15px;
    padding-top: 10px;
    vertical-align: middle;
    width: 298px;
    cursor: pointer;
    height:32px;
    margin-left:0px;
    font-size: 11px;
    font-weight: normal;
    color: #77787A ;
    white-space: nowrap;
    text-align: left;
    text-transform: capitalize;
    background-color: #FFFFFF;

}

.pf_filter_cl01 .pf_content_menu a:active,.pf_filter_cl01 .pf_content_menu a:hover {/*filter menu container dropdown*/
    background-color: #bbdc9b;

}


.pf_filter_cl02 {/*sub filter menu container dropdown*/
    top:45px;
    z-index: 500;

}

.pf_filter_cl02 .pf_content_menu a{/*sub filter menu items*/
    width:100%;
    padding-left: 5px;
    padding-top: 8px;
    vertical-align: middle;
    width: 298px;
    cursor: pointer;
    height:30px;
}

.pf_filter_cl03{/*main  filter menu container with arrow image*/

    width: 100%;
    padding: 10px 0px 0px 0px;
    font-size: 12px;
    font-weight:bold;
    height:35px;
    color: #FFFFFF;
    text-align: center;
    background-color: #abd6cf;
    position: relative;
    cursor: pointer;

}

.pf_filter_cl03:active{/*active state*/

    background-color: #019d8c;

}

.pf_filter_cl05{
    height:auto;

}

.pf_filter_cl05 .pf_filter_cl04{/*expand collapse  arrow */
    background-image: url("../images/img_expand.png");

}

.pf_filter_cl04 {/*expand collapse  arrow */
    position: absolute;
    right:10px;
    top:15px;
    height:15px;
    width:15px;
    background-image: url("../images/img_collapse.png");
    background-repeat: no-repeat;
    cursor: pointer;

}

.pf_filter_cl06 {/*sort by green background */
    background-color: #bbdc9b;

}

.pf_filter_cl06:active {/*sort by green background active */
    background-color: #a2d178;

}

/********filter menu responsive ends********/

