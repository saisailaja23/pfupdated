/* site specific styles starts */

* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
}

html{
    /*min-height:100%;
    position:relative;*/

}

body{ /* body styles */
    background-color:#FFFFFF;
    font-family: Helvetica, Arial, "Lucida Grande", sans-serif;
    height:100%;
    font-size: 12px;
    line-height:16px;
    margin:0px;
    padding:0px;
    color:#76787b;

}

/*#header {
    display:none;
}*/

.sys_main_content {
    width: 100%;
}

#footer_content {
    width: 100%;
}

.clear{/* clear*/
    clear:both;

}

a:link, a:visited{/*links*/
    color: #f195bf;
    text-decoration: none;
}

a:hover,  a:active{/*link styles*/
    color: #a2a4a7;
    text-decoration: none;
}

/* public home page starts  */


#wrap{/* outer container*/
    width: 1300px;
    margin: 0 auto;

}

#main{/* main container*/
    float: left;
    width: 1028px;
    position:relative;

}


#side{/*right side container*/
    float: right;
    width: 265px;
    background: #e2f0d4;
    min-height: 1000px;
    height: 100%;
    position:relative;

}

.home{
    width:1300px !important;
}

/* header styles */

header{/*header container*/
    height: 140px;
    width: 100%;

}

.pf_logo { /* logo*/
    display: block;
    float: left;
    margin-left:165px;
    margin-top: 30px;
}

#pf_header_id01{/*login and join button in for mobile devices*/
    height:37px;
    width:100%;
    display:none;
}

#pf_header_id02{/*text*/
    font-size: 17px;
    line-height: 16px;
    font-weight: bold;
    float: left;
    margin-left: 255px;
    margin-top: 12px;
    position:relative;
    top:8px;
}

#pf_header_id03 {/*button container*/
    width: 124px;
    height: 55px;
    float: right;
    margin-top: -25px;
    position: relative;
    right: 0;
}


#pf_header_id04 {/* dropdown icon */
    width: 124px;
    height: 55px;
    float: right;
    margin-top: -30px;
    position: relative;
    bottom: 0;
    right: 0;
    position: relative;
}

.pf_header_cl01 {/* join button */
    margin-bottom: 5px;
    background-color: #bbdc9b;
}

.pf_header_cl02{/* login button */
    background-color: #abd6cf;
}


.pf_header_cl01:hover {/* join button hover*/
    background-color: #a2d178;
}

.pf_header_cl02:hover {/* login button hover*/
    background-color: #019d8c;
}

.pf_header_cl01, .pf_header_cl02 {/* button styles*/
    position: relative;
    text-align: center;
    padding: 8px 7px;
    font-size: 12px;
    text-transform: uppercase;
    color: #fff;
    font-weight: bold;
    cursor: pointer;
    height:28px;
}

.pf_header_cl03 {/* dropdown icon*/
    position: relative;
    background-image: url("../images/img_dropdown.png");
    background-repeat: no-repeat;
    height:32px;
    width:32px;
    cursor: pointer;
    display:none;
}

.pf_header_cl03:active {/* dropdown icon*/

    background-image: url("../images/img_dropdown_select.png");

}

.pf_header_cl05{/* join button */
    background-color: #bbdc9b;
}

.pf_header_cl05:active{/* join button active*/
    background-color: #a2d178;
}

.pf_header_cl05:hover{/* join button hover*/
    background-color: #a2d178;
}

.pf_header_cl06{/* login button */
    background-color: #abd6cf;
}

.pf_header_cl06:active{/* login button active*/
    background-color:  #019d8c;
}

.pf_header_cl05, .pf_header_cl06 {/* button styles for devices*/
    position: relative;
    text-align: center;
    padding: 8px 7px;
    font-size: 12px;
    text-transform: uppercase;
    color: #fff;
    font-weight: bold;
    cursor: pointer;
    float:right;
    width:30%;
    height:35px;
    margin-left: 1%

}

.pf_header_cl07{/* dummy container in login and join section devices*/
    width:38%;
    background-color:#d3e9e6;
    position:relative;
    height:35px;
    padding:0px 10px 0px 0px;
}

.pf_header_cl08{/* dummy container in logout*/
    width:74.5%;
    background-color:#57b4a8;
    position:relative;
    height:35px;
    padding:8px 10px 0px 15px;
}

.pf_header_cl08 span{/* dummy container text*/
    font-size: 13px;
    color:#FFFFFF;
}

#pf_banner{ /* splash image */
    width: 100%;
    height: 441px;
    background-image: url("../images/img_splash.png");
    background-repeat: no-repeat;
    margin-bottom: 5px;
    overflow: hidden;
    background-size: 100%;
}

#pf_content_side{
    display: none;
}

#pf_side_id01{/*data container*/
    width: 180px;
    margin: 0 auto;
    height:auto;
    padding:3px 0px 0px 0px;
    overflow: hidden;
    position:relative;

}

#pf_side_id02{/*data inner container*/
    width: 100%;
    height:auto;
    padding:3px 0px 0px 0px;
}

a.pf_side_cl01{ /* view all families */
    background: #BBDC9B;
    border: 2px solid #FFFFFF;
    color: #FFFFFF;
    cursor: pointer;
    display: block;
    font-size: 14px;
    font-weight: bold;
    height: 35px;
    line-height: 29px;
    text-align: center;
    width: 180px;
    margin: 74px auto 25px
}

a.pf_side_cl01:hover{
    background-color: #a2d178;
    text-decoration: none;
    color: #FFFFFF;
}

.pf_side_cl02{/*data container*/
    width: 180px;
    height: 140px;
    padding: 3px 3px 3px 3px;
    text-align: center;
    background-color: #FFFFFF;
    margin-bottom: 8px
}

.pf_side_cl02 img{/*image*/
    width: 174px;
    height: 134px;

}


#pf_content_side{/*view our families responsive conatiner*/
    width: 100%;
    background: #e2f0d4;
    height: auto;
    position:relative;

}

a.pf_content_side_cl01{ /* view all families responive*/
    background: #BBDC9B;
    border: 2px solid #FFFFFF;
    color: #FFFFFF;
    cursor: pointer;
    display: block;
    font-size: 12px;
    font-weight: bold;
    height: 35px;
    line-height: 29px;
    text-align: center;
    width: 170px;
    margin-left:15px;
    position: relative;
    top:15px;
}

a.pf_content_side_cl01:hover, a.pf_content_side_cl01:active{
    background-color: #a2d178;
    text-decoration: none;
    color: #FFFFFF; 
}

#pf_content_side #pf_side_id01{/* view all families responive*/
    width:auto;
    margin:20px 7px 0px 15px;
    height: auto;
    padding-bottom: 8px;
}

#pf_content_side #pf_side_id02{
    width:100%;
    height: auto;
    position: relative;
}


#pf_content_side .pf_side_cl02{
    float:left;
    width:115px;
    height:86px;
    margin-right: 10px;
    margin-bottom: 10px;
}


#pf_content_side .pf_side_cl02 img{
    width:110px;
    height:80px;

}

#pf_content_id01{/*content section */
    position: relative;
    margin: 0 0 0 163px;
}

#pf_content_id02{/*icons container*/
    position: relative;
    width: 30px;
    float: left;
}

#pf_content_id02 a{/*icons*/
    position: relative;
    height: 28px;
    width: 29px;
    margin-bottom: 5px;
    display: block;
    background-repeat: no-repeat;
}

#pf_content_id03{/*sub menu section and paragraph container*/
    position: relative;
    width: 832px;
    float: right;
    height: 170px;
}

.pf_content_menu{/*sub menu section */
    position: relative;
    width: 100%;
    height: 30px;
}

.pf_content_menu a{/*sub menu links */
    position: relative;
    float: left;
    width: 203px;
    padding: 6px 0px 6px 0px;
    text-align: center;
    text-transform: uppercase;
    font-size: 12px;
    color: #FFFFFF;
    margin-left: 5px;
    font-weight: bold;
    display: block;
    text-decoration: none;
    background-color: #abd6cf;
}

.pf_content_menu a:hover{/*sub menu links hover*/
    background-color: #019d8c;
}

.pf_content_menu a.last{/*sub menu last link */
    background-color: #f5b3d1;
}

.pf_content_menu a.last:hover{/*sub menu last link hover */
    background-color: #f195bf;
}

.pf_content_cl01{/*content paragraph*/
    padding: 12px 20px 0 25px;
    width:auto;
    text-align: justify;
}

.pf_content_cl02{/*fb icon*/
    background-image: url("../images/icon_fb.png");
}

.pf_content_cl02:hover, .pf_content_cl02:active{/*fb hover icon*/
    background-image: url("../images/icon_fb_active.png");
}


.pf_content_cl03{/*twitter icon*/
    background-image: url("../images/icon_tw.png");
}

.pf_content_cl03:hover, .pf_content_cl03:active{/*twitter hover icon*/
    background-image: url("../images/icon_tw_active.png");
}

.pf_content_cl04{/*google plus icon*/
    background-image: url("../images/icon_go.png");
}

.pf_content_cl04:hover, .pf_content_cl04:active{/*google plus hover icon*/
    background-image: url("../images/icon_go_active.png");
}

.pf_content_cl05{/*pi icon*/
    background-image: url("../images/icon_pi.png");
}

.pf_content_cl05:hover, .pf_content_cl05:active{/*pi  hover icon*/
    background-image: url("../images/icon_pi_active.png");
}

.pf_content_cl06{/*bi icon*/
    background-image: url("../images/icon_bi.png");
}

.pf_content_cl06:hover, .pf_content_cl06:active{/*bi hover icon*/
    background-image: url("../images/icon_bi_active.png");
}

#pf_footer{/*footer container*/
    position:relative;
    float:right;
    margin:15px 0px 15px 0px;
    color: #B4DA91;
    font-size: 11px;
    padding-right: 25px;
}

#pf_footer a{/*footer links*/
    font-weight: bold;
    margin-right:5px;
}


/* nav menu modal dialog starts*/
#navmenu {
    top: 0%;
    position: absolute;
}

#navmenu .modal-dialog {
    margin: 102px 0px 0px 0px;
}

.modal-backdrop{
    background-color: #FFFFFF;
}

#navmenu .modal-content {
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border: 0px solid #999;
    border: 0px solid rgba(0,0,0,.2);
    border-radius: 0px;
    outline: 0;
    background:none;
    -webkit-box-shadow: 0 0px 0px rgba(0,0,0,0);
    box-shadow: 0 0px 0px rgba(0,0,0,0);
}

#navmenu .modal-body {
    padding: 0px 0px 0px 0px;
    background-color:#FFFFFF;
    height:132px;
}

#navmenu .pf_content_menu{
    display: block;
    background-color: #FFFFFF;
}

#navmenu .pf_content_menu span{
    position: absolute;
    display: block;
    height:8px;
    width:15px;
    top:-8px;
    right:40px;
    background-image: url("../images/img_arrow_green.png");
    background-repeat: no-repeat;
}

#navmenu .pf_content_menu a{
    float:none;
    width:100%;
    padding:7px 0px 7px 15px;
    text-align: left;
    margin-bottom: 3px;
    margin-left: 0px;
}

body.modal-open,
.modal-open .navbar-fixed-top,
.modal-open .navbar-fixed-bottom {
    margin-right: 15px;
}

body.modal-open {margin-right: 0px}

/*nav menu modal dialog ends*/

/* popup modal dialog starts*/

.popup {

    top:50%;
    margin-top:-75px;

}

.popup .modal-dialog {

    width:275px;
    margin: auto;

}

.popup .modal-content {
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    border-radius: 0px;
    outline: 0;
    border:0px solid rgba(0,0,0,.2);
    background:none;
    -webkit-box-shadow: 0 0px 0px rgba(0,0,0,0);
    box-shadow: 0 0px 0px rgba(0,0,0,0);
}

.popup .modal-body {
    padding: 0px 0px 0px 0px;
    font-family: Helvetica, Arial, "Lucida Grande", sans-serif;
    font-size: 12px;
    line-height:16px;
    margin:0px;
    padding:0px;
    color:#76787b;
}

.popup .modal-body .closebtn {
    position: absolute;
    right:0px;
    top:0px;
    height:24px;
    width:24px;
    background-image: url("../images/img_close.png");
    background-repeat: no-repeat;
    z-index: 1000;
    cursor: pointer;
}

.popup .modal-body .pf_popupcl01{
    position: relative;
    padding:15px;
    height:150px;
    background-color:#FFFFFF;
    border: 4px solid #7AC142 ;
    overflow: auto;
    overflow-x:hidden;
    top:10px;
}

/*********login dialog starts************/
#login {
    margin-top:-150px;
}

#login .modal-body .pf_popupcl01{
    overflow: hidden;
    height:300px;
}

#login .modal-dialog {

    width:532px;
    margin: auto;

}

.pf_popup_login_cl01{/*login text*/
    position: relative;
    font-size: 30px;
    text-transform: uppercase;
    color: #a2d077;
    font-weight: bold;
    padding: 27px 0px 0px 55px;
    top: 30px;

}

.pf_popup_login_cl02{/*profile text*/
    position: relative;
    font-size: 12px;
    font-weight: bold;
    text-transform: uppercase;
    color: #009D8C;
    padding: 0px 0px 0px 55px;
    margin-top: 40px;
    margin-bottom: 10px;

}

.pf_popup_login_cl03{/*username container*/
    position: relative;
    height:30px;
    width:390px;
    margin:auto;

}

.pf_popup_login_cl03 label{/*username text*/
    font-weight: normal;
    float: left;
    margin-right: 50px;
    font-size: 12px;

}

.pf_popup_login_cl03 input{/*username text*/
    font-weight: normal;
    position: relative;
    width: 235px;
    height:22px;
    padding: 2px;
    background-color: #fff;
    border: 1px solid  #8b8c8f;
    margin:0px;
    outline: none;
    display: block;

}

.pf_popup_login_cl04 {/*remember me container*/
    position: relative;
    width: 312px;
    height: auto;
    float: right;
}

.pf_popup_login_cl05 {/*checkbox*/
    position: relative;
    width:75%;
    height:30px;
    margin-top:5px;

}

.pf_popup_login_cl06 {/*checkbox image*/
    position: relative;
    width:18px;
    height:18px;
    background-image: url("../images/img_checkbox.png");
    background-repeat: no-repeat;
    float: left;

}

.pf_popup_login_cl05 span {/*checkbox text*/
    position: relative;
    display: block;
    float:left;
    margin-top:2px;
    margin-left:10px;
    font-size: 12px;

}

.pf_popup_login_cl07 {/*checkbox checked image*/
    position: relative;
    width:18px;
    height:18px;
    float:left;
    background-image: url("../images/img_checked.png");
    background-repeat: no-repeat;

}

.pf_popup_login_cl08 {/*Login button*/
    position: relative;
    padding:5px 15px;
    float:left;
    text-align: center;
    color:#FFFFFF;
    font-size: 12px;
    font-weight: bold;
    height:27px;
    cursor: pointer;
    background-color:#f195bf;

}


.pf_popup_login_cl05 span.pf_popup_login_cl09 {/*forgot password*/

    float:right;
    cursor: pointer;
    margin-top: 3px;
}

.pf_popup_login_cl05 span.pf_popup_login_cl010 {/*not a member*/

    margin-left: 0px;
    margin-top: 3px;
}


.pf_popup_login_cl05 span.pf_popup_login_cl011 {/*join now*/

    float:right;
    margin-top: 3px;
    color:#009D8C;
    cursor: pointer;
}

.pf_popup_login_cl05 span.pf_popup_login_cl011:hover,.pf_popup_login_cl05 span.pf_popup_login_cl09:hover ,.pf_popup_login_cl05 span.pf_popup_login_cl011:active,
.pf_popup_login_cl05 span.pf_popup_login_cl09:active {/*join now*/

    color: #f195bf;
    text-decoration: underline;;
}


/*********login dialog starts************/

/*popup modal dialog ends*/

/* public home page ends  */


/* view our home page starts */
.view{
    width:1010px !important;
}

.pf_view_logo{/*header logo */
    float:left;
    display:block;
    margin-top:-20px;
    background-image: url("../images/img_logo_view.png");
